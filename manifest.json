{
  "manifest_version": 3,
  "name": "Social Media Concealer",
  "description": "Conceals distracting elements on various social media websites",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/logo.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["*://*.youtube.com/*"]
    }
  ]
}
